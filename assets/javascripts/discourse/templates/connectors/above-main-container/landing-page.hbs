    </div>
  </div>

  <div class="grid-center">
    <div class="col-10">
        <div class="custom-button-2">
          <a href="mailto:onedcu@dcu.ie" class="custom-button-link">
          Having trouble logging in?
          <br>
          Contact us at onedcu@dcu.ie
        </a>
        </div>
    </div>

    <div class="col-5">
      
        <div class="custom-button-2">
          <a href="/plugins/OneDCU-Landing-Page-Plugin/files/onedcu pdf placeholder.pdf" target="_blank" class="custom-button-link">
            Schedule for the 9th of May
          </a>
        </div>
      
    </div>

    <div class="col-5">
      
        <div class="custom-button-2">
          <a href="/plugins/OneDCU-Landing-Page-Plugin/files/onedcu pdf placeholder.pdf" target="_blank" class="custom-button-link">
            Schedule for the 9th of May
          </a>
        </div>
      
    </div>
  </div>



  
  <!-- Clock -->
  <div id="clock-main" class="grid-center py-5 no-display main-text-color">
    
    <div class="col-10 grid-center">
    	<!-- Days -->
    	<div class="col text-center my-1" id="clock-main-days-col">
        <div class="px-1 py-1">
      		<div id="clock-days-value" class="font-size-6 clock-time-value">
      			00
      		</div>
      		<div class="font-size-1 clock-time-desc">
      			Days
      		</div>
        </div>
    	</div>

    	<!-- Hours -->
    	<div class="col text-center my-1">
        <div class="px-1 py-1">
      		<div id="clock-hours-value" class="font-size-6 clock-time-value">
      			00
      		</div>
      		<div class="font-size-1 clock-time-desc">
      			Hours
      		</div>
        </div>
    	</div>

    	<!-- Minutes -->
    	<div class="col text-center my-1">
    		<div class="px-1 py-1">
          <div id="clock-minutes-value" class="font-size-6">
      			00
      		</div>
      		<div class="font-size-1 clock-time-desc">
      			Minutes
      		</div>
        </div>
    	</div>

    	<!-- Seconds -->
    	<div class="col text-center my-1">
        <div class="px-1 py-1">
      		<div id="clock-seconds-value" class="font-size-6 clock-time-value">
      			00
      		</div>
      		<div class="font-size-1 clock-time-desc clock-time-value">
      			Seconds
      		</div>
        </div>
    	</div>
    </div>

	</div>

	<!-- Events -->
	<div id="events-main" class="grid py-5 no-display">

		<div class="col-4_xs-12_sm-6">
      {{event-list events=liveEvents label="Now On"}}
    </div>

    
    <div class="col-4_xs-12_sm-6">
      {{event-list events=nextEvents label="Coming Up"}}
    </div>

    
    <div class="col-4_xs-12_sm-12">
      <div class="otherItemList">

        <!-- Placeeholder becausee of spacing -->
        <div class="eventListTitle" style="visibility: hidden;">
          <span class="listText">Placeholder</span>
        </div>

        <div id="media-slideshow" style="padding-top: 17px;">
          <div>
            <img class="media-100-width" src="/plugins/OneDCU-Landing-Page-Plugin/images/brian.png" />
          </div>

          <div class="no-display">
            <img class="media-100-width" src="/plugins/OneDCU-Landing-Page-Plugin/images/beardy.png" />
          </div>

          <div class="no-display">
            <img class="media-100-width" src="/plugins/OneDCU-Landing-Page-Plugin/images/johnny.png" />
          </div>
        </div>
    
        

